PROJECT_NAME           = "edipack"
OUTPUT_DIRECTORY       = ../_build/doxygen
GENERATE_XML           = YES
XML_OUTPUT             = xml
INPUT                  = ../src/c_bindings
RECURSIVE              = YES
GENERATE_HTML          = NO
GENERATE_LATEX         = NO
QUIET                  = YES
EXTRACT_ALL            = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
FILE_PATTERNS          = *.h *.hpp *.c *.cpp *.h.in
ALIASES += rst="\verbatim embed:rst"
ALIASES += endrst="\endverbatim"
